<!doctype html>
<html lang="ru">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>UseTech - Главная</title>
    <link rel="stylesheet" href="css/fractionslider/fractionslider.css">
    <link rel="stylesheet" href="css/main.css">
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="hide">
        {{>svg-icons}}
    </div>
    <div class="container">
        <!-- Site header -->
        {{>header}}
        <!-- Slider -->
        <div class="row row_grey">
            <div class="col-md-12">
                {{>main-slider}}
            </div>
        </div>
        <!-- Main content -->
        <main class="content">
            <div class="row row_grey">
                <div class="col-md-8">
                    {{>about}}
                </div>
                <div class="col-md-4">
                    {{>events}}
                </div>
            </div>
            <!-- Clients slider -->
            <div class="slider slider_clients">
                clients slider
            </div>
        </main>
        <!-- Site footer -->
        <div class="row">
            <div class="col-md-12">
                <div class="row">
                    {{>footer}}
                </div>
            </div>
        </div>
    </div>
    <script src="vendors/plugins.min.js"></script>
    <script src="js/main.js"></script>
</body>
</html>